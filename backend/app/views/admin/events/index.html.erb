<h1>Event log</h1>
<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th>Action</th>
      <th>Object</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.created_at.strftime('%A %e %B %Y, %l:%M%P') %></td>
        <td><%= event.user.try(:name) %></td>
        <td><%= event.action %></td>
        <td><pre><%= event.context && JSON.pretty_generate(event.context.as_json) %></pre></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @events %>
